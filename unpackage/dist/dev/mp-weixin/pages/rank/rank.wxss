	    -ms-flex: 1;
	        flex: 1;
}
.rankberfthree .list .center{
	width:100%;
	text-align: center;
	position: relative;
}
.rankberfthree .list .center .hearcat{
	width: 64rpx;
	height: 54rpx;
	position: absolute;
	top: -40rpx;
	left: 50%;
	margin-left: -32rpx;
}
